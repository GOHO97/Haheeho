const contentTd = document.getElementById("boardContentTd");
const sequenceNumber = document.getElementById("sequenceNumber").innerText;
const getContentURL = "http://sdgn-djvemfu.tplinkdns.com:6776/board.get.content"

function getContentFromMongo() {
	console.log(sequenceNumber);
	$.ajax({
		type: "POST",
		url: getContentURL,
		data: {b_number: "28"},
		success: (result) => {
			alert(JSON.stringify(result));
			//$("#boardContentTd").innerHTML(result["content"]);
		}
	});
}

contentTd.addEventListener('load', () => {getContentFromMongo()});